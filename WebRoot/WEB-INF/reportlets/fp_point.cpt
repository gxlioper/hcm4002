<Query>
<![CDATA[select distinct cis.id, cis.title_info,cis.invoice_num,cis.invoice_amount,cis.invoice_maker,uu.chi_name,
[dbo]A.[xx_dx]A(cis.invoice_amount,0) as DXJE,dd.data_name,css.amount1,css.amount2,
cis.invoice_time,e.exam_num, c.user_name, dbo.GetComanyByComID(e.customer_id) as Company, c.id_num, c.sex, e.age 
from customer_info c, exam_info e ,charging_invoice_single cis ,charging_summary_single css,
charging_way_single sws,user_usr uu,data_dictionary dd 
where c.id=e.customer_id and css.exam_id = e.id and cis.invoice_status = 'Y' 
and css.invoice_id = cis.id and css.id=sws.summary_id and sws.charging_way=dd.id and cis.invoice_maker=uu.id
and sws.charging_way in(select dd.id from data_dictionary 
where data_dictionary.data_code ='SFFSLX'  and data_dictionary.data_name in('现金','银行卡'))and cis.invoice_num = '${a}'
]]></Query>
</TableData>
</TableDataMap>
